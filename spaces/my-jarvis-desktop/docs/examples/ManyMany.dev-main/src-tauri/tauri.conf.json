{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ManyMany.dev",
  "version": "0.1.25",
  "identifier": "com.jasonzhou.manymany",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "ManyMany.dev",
        "width": 1400,
        "height": 900,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        {
          "identifier": "main-capability",
          "description": "Capability for main window",
          "windows": ["main"],
          "permissions": [
            "core:default",
            "core:event:allow-listen",
            "core:event:allow-emit",
            "dialog:allow-open",
            "dialog:default",
            "updater:default",
            "updater:allow-check",
            "updater:allow-download-and-install",
            "opener:default",
            "opener:allow-open-url"
          ]
        }
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "signingIdentity": null,
      "hardenedRuntime": true,
      "entitlements": null
    }
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/JayZeeDesign/ManyMany.dev/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDEyRTA1NzYwNzBDNzA1RTkKUldUcEJjZHdZRmZnRWhYUFR2VmRyMXR0UlRiSVM4WXp6eDhUK2x3eXNDNTNNN0hwVmRFdUt2K3YK"
    }
  }
}
